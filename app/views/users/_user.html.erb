<div id="<%= dom_id user %>">
  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>First name:</strong>
    <%= user.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= user.last_name %>
  </p>

  <p>
    <strong>Crypted password:</strong>
    <%= user.crypted_password %>
  </p>

  <p>
    <strong>Salt:</strong>
    <%= user.salt %>
  </p>

</div>
